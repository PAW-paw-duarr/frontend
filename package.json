{
  "name": "frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 3001",
    "build": "vite build && tsc --noEmit",
    "serve": "vite preview",
    "start": "vite",
    "typecheck": "react-router typegen && tsc",
    "openapi": "npx openapi-typescript https://raw.githubusercontent.com/PAW-paw-duarr/openapi/refs/heads/main/openapi.json -o ./src/lib/api-schema.d.ts",
    "prepare": "husky",
    "check": "tsc --noEmit && npm run lint",
    "lint": "eslint src/** --ext .ts,.tsx",
    "lint:fix": "eslint src/** --ext .ts,.tsx --fix",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@mantine/hooks": "^8.2.8",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.1.16",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.135.2",
    "@tanstack/react-router-devtools": "^1.135.2",
    "@tanstack/router-plugin": "^1.135.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "liquid-glass-react": "^1.1.1",
    "lucide-react": "^0.548.0",
    "minisearch": "^7.2.0",
    "next-themes": "^0.4.6",
    "openapi-fetch": "^0.14.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.65.0",
    "react-icons": "^5.5.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.16",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^16.1.5",
    "openapi-typescript": "^7.9.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "npm run check"
  },
  "prettier": {
    "printWidth": 120,
    "useTabs": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "jsxSingleQuote": false,
    "bracketSameLine": false,
    "endOfLine": "lf",
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }
}
